<launch>
    <arg name="robot_name" default="alpha_img"/>

    <!-- Bring up the Bluerobotics Pressure sensor -->
    <include file="$(find alpha_img_bringup)/launch/include/bluerobotics_bar30.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include>

    <!-- Bring up the Power Minotor  -->
    <include file="$(find alpha_img_bringup)/launch/include/power_monitor.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include>

    <!-- Bring up Xsens AHRS -->
    <!-- <include file="$(find alpha_img_bringup)/launch/include/xsens_imu.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include>  -->

    <!-- Bring up the microstrain ahrs  -->
    <include file="$(find alpha_img_bringup)/launch/include/microstrain.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include> 

    <!-- Bring up madgwick filter-->
    <include file="$(find alpha_img_bringup)/launch/include/madgwick_filter.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include> 

    <!-- Bring up GPS --> 
    <include file="$(find alpha_img_bringup)/launch/include/gps_nmea.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include>

    <!-- Bringup computer monitor node-->
    <include file="$(find alpha_img_bringup)/launch/include/monitor.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include>

    <!-- Bring up Waterlinked DVL -->
    <include file="$(find alpha_img_bringup)/launch/include/waterlinked_dvl.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include>
    
</launch>  
