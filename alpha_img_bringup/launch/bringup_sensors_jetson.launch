<launch>
  <arg name="robot_name" default="alpha_img"/>

  <!-- Bring up the Blueprint Oculus FLS sensor -->
  <include file="$(find alpha_img_bringup)/launch/include/blueprint_oculus_fls.launch.xml">
    <arg name="robot_name" value="$(arg robot_name)"/>
  </include>  

  <!-- Bring up the Bluerobotics Ping360 MSIS sensor -->
  <include file="$(find alpha_img_bringup)/launch/include/bluerobotics_ping360.launch.xml">
    <arg name="robot_name" value="$(arg robot_name)"/>
  </include>  

  <!-- Bring up the DWE stellarHD fisheye camera sensor -->
  <include file="$(find alpha_img_bringup)/launch/include/dwe_fisheye_cam.launch.xml">
    <arg name="robot_name" value="$(arg robot_name)"/>
  </include>  

</launch>  