
<launch>

    <arg name="robot_name" default="alpha_img"/>

    <node ns="$(arg robot_name)" pkg="imu_filter_madgwick" type="imu_filter_node" 
          name="imu_filter_node" output="screen">
          
        <param name = "world_frame" value="enu"/>
        <param name = "gain" value = "0.4"/>
        <!-- <param name = "mag_bias_x" value="-0.28812"/> -->
        <!-- <param name = "mag_bias_y" value="-0.0252"/> -->
        <!-- <param name = "mag_bias_z" value="0.42527816"/> -->
        <param name = "gyro_bias_x" value="-0.002047483082430453"/>
        <param name = "gyro_bias_y" value="-0.0011376166483670807"/>
        <param name = "gyro_bias_z" value="5.829118179724444e-05"/>
        <param name = "accel_bias_x" value="0.0001292"/>
        <param name = "accel_bias_y" value="0.000424247"/>
        <param name = "accel_bias_z" value="-0.00362847"/>
        <param name = "use_mag" value = "true" />
        <param name = "publish_tf" value ="false"/>
    </node>

</launch>